{
  "name": "@test/util",
  "imports": {
    "#util/*": [
      "./src/*",
      "./src/*.ts"
    ]
  },
  "exports": {
    "./util": "./src/util.ts",
    "./works": {
      "import": [
        "./src/util.ts"
      ],
      "types": "./src/*.d.ts",
      "default": [
        "./src/util.ts"
      ]
    },
    "./notworks": {
      "types": "./src/*.d.ts",
      "default": [
        "./src/util.ts"
      ]
    }
  },
  "devDependencies": {
    "@test/tsconfig": "workspace:^"
  }
}
